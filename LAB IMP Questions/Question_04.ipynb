   "source": [
    "#Prove that if u = e^x(xcos(y) - ysin(y)) then Uxx + Uyy = 0.\n",
    "from sympy import *\n",
    "x,y=symbols ('x y')\n",
    "u=exp(x)*(x*cos(y)-y*sin(y))\n",
    "display(u)\n",
    "dux=diff(u,x)\n",
    "duy=diff(u,y)\n",
    "uxx=diff(dux,x)\n",
    "uyy=diff(duy,y) \n",
    "w=uxx+uyy    \n",
    "w1=simplify (w)\n",
    "print ('Ans:', float (w1))"
   ]
  }
 ],
